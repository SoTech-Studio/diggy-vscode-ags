{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "AGS",
  "scopeName": "source.ags",
  "patterns": [
    {
      "begin": "^(\"GROUP\")",
      "end": "$",
      "beginCaptures": {
        "1": { "name": "keyword.control.group.ags" }
      },
      "contentName": "entity.name.class.ags",
      "patterns": [
        { "match": ",", "name": "punctuation.separator.comma.ags" }
      ]
    },
    {
      "begin": "^(\"HEADING\")",
      "end": "$",
      "beginCaptures": {
        "1": { "name": "keyword.control.heading.ags" }
      },
      "contentName": "variable.other.heading.ags",
      "patterns": [
        { "match": ",", "name": "punctuation.separator.comma.ags" }
      ]
    },
    {
      "begin": "^(\"UNIT\")",
      "end": "$",
      "beginCaptures": {
        "1": { "name": "keyword.control.unit.ags" }
      },
      "contentName": "meta.unit.ags",
      "patterns": [
        { "match": ",", "name": "punctuation.separator.comma.ags" }
      ]
    },
    {
      "begin": "^(\"TYPE\")",
      "end": "$",
      "beginCaptures": {
        "1": { "name": "keyword.control.type.ags" }
      },
      "contentName": "meta.type.ags",
      "patterns": [
        { "match": ",", "name": "punctuation.separator.comma.ags" }
      ]
    },
    {
      "begin": "^(\"DATA\")",
      "end": "$",
      "beginCaptures": {
        "1": { "name": "keyword.control.data.ags" }
      },
      "contentName": "source.data.ags",
      "patterns": [
        { "match": ",", "name": "punctuation.separator.comma.ags" }
      ]
    },
    {
      "match": ".",
      "name": "text.ags"
    }
  ]
}
